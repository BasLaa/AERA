#ifndef __HASH_CONTAINERS_H
#define __HASH_CONTAINERS_H
#include "../r_code/types.h"

#ifndef LINUX
namespace __gnu_cxx {
	template<class T>
		struct hash<const T*>
		{
			size_t operator()(const T* __p) const
			{ return reinterpret_cast<size_t>(__p); }
		};
	template<class T>
		struct hash<T*>
		{
			size_t operator()(T* __p) const
			{ return reinterpret_cast<size_t>(__p); }
		};
}
#endif

#endif
